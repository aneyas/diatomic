Cell[BoxData[
 RowBox[{
  RowBox[{"CalcEDiff", "[", 
   RowBox[{"lm_", ",", "n_", ",", "pstart_", ",", "ZR_List"}], "]"}], ":=", 
  RowBox[{
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"First", "[", "#", "]"}], ",", 
      RowBox[{"Abs", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"-", "2"}], 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{
            RowBox[{"Last", "[", "#", "]"}], "/", 
            RowBox[{"First", "[", "#", "]"}]}], ")"}], "2"]}], "+", 
        FractionBox["1", 
         RowBox[{"First", "[", "#", "]"}]], "+", 
        FractionBox["1", 
         RowBox[{"2", 
          SuperscriptBox["n", "2"]}]]}], "]"}]}], "}"}], "&"}], "/@", 
   RowBox[{"CalcP", "[", 
    RowBox[{"lm", ",", "pstart", ",", "ZR"}], "]"}]}]}]], "Input"],

