       RowBox[{
        RowBox[{"-", "2"}], 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{
           RowBox[{"Last", "[", "#", "]"}], "/", 
           RowBox[{"First", "[", "#", "]"}]}], ")"}], "2"]}], "+", 
       FractionBox["1", 
        RowBox[{"First", "[", "#", "]"}]]}]}], "}"}], "&"}], "/@", 
   RowBox[{"CalcP", "[", 
    RowBox[{"lm", ",", "pstart", ",", "ZR"}], "]"}]}]}]], "Input"],

