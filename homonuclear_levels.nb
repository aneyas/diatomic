}], "Section"],

Cell[BoxData[
 RowBox[{
  RowBox[{"CalcP", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"l_", ",", "m_"}], "}"}], ",", "pstart_", ",", "ZR_List"}], 
   "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"p0", "=", "pstart"}], "}"}], ",", 
    RowBox[{
     RowBox[{
      RowBox[{"{", 
       RowBox[{"#", ",", 
        RowBox[{"p0", "=", 
         RowBox[{
          RowBox[{"FindRoot", "[", 
           RowBox[{
            RowBox[{"RadialPZero", "[", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{"l", ",", "m", ",", "p"}], "}"}], ",", 
              RowBox[{"{", 
               RowBox[{
                RowBox[{"2", " ", "#"}], ",", "0"}], 
               RowBox[{"(*", "homonuclear", "*)"}], "}"}], ",", "10"}], "]"}],
             ",", 
            RowBox[{"{", 
             RowBox[{"p", ",", "p0"}], "}"}]}], "]"}], "\[LeftDoubleBracket]", 
          RowBox[{"1", ",", "2"}], "\[RightDoubleBracket]"}]}]}], "}"}], 
      "&"}], "/@", "ZR"}]}], "]"}]}]], "Input"],

Cell[BoxData[
 RowBox[{
  RowBox[{"CalcE", "[", 
   RowBox[{"lm_", ",", "pstart_", ",", "ZR_List"}], "]"}], ":=", 
  RowBox[{
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"First", "[", "#", "]"}], ",", 
      RowBox[{
       RowBox[{"-", "2"}], 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{
          RowBox[{"Last", "[", "#", "]"}], "/", 
          RowBox[{"First", "[", "#", "]"}]}], ")"}], "2"]}]}], "}"}], "&"}], "/@", 
   RowBox[{"CalcP", "[", 
    RowBox[{"lm", ",", "pstart", ",", "ZR"}], "]"}]}]}]], "Input"],

Cell[BoxData[
 RowBox[{
  RowBox[{"CalcErepul", "[", 
   RowBox[{"lm_", ",", "pstart_", ",", "ZR_List"}], "]"}], ":=", 
